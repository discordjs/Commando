		"require-all": "^3.0.0"
	},
	"devDependencies": {
		"@types/node": "^12.7.3",
		"discord.js-docgen": "discordjs/docgen",
		"eslint": "^6.3.0",
		"typescript": "^3.6.2"
	},
	"peerDependencies": {
		"@types/better-sqlite3": "^5.0.0",
		"better-sqlite3": "^5.0.0",
		"discord.js": "^12.0.0",
		"sqlite": "^3.0.0"
	},
	"engines": {
		"node": ">=8.6.0"
	}
